@model Ignis.Models.VehicleDamageStatus

@{
    ViewBag.Title = "ViewClosedTicket";
}

<h2>View Closed Ticket</h2>

<h4>Vehicle Damage</h4>
@{Html.RenderAction("ViewVehicleDamage", new { id = Model.VehicleDamage.Id });}
<hr />

<div class="row">
    <h4>File Movement</h4>
    @{Html.RenderAction("ViewFileMovement", new { id = Model.CaseId });}
</div>
<hr />

<div class="row">
    <h4>Vehicle Details</h4>
    @{Html.RenderAction("ViewVehicle", new { id = Model.VehicleDamage.VehicleId });}
</div>
<hr />

<div class="row">
    <h4>Vehicle Damage History</h4>
    @{Html.RenderAction("ViewPreviousDamages", new { vehicleId = Model.VehicleDamage.VehicleId, vehicleDamageTypeId = Model.VehicleDamage.VehicleDamageTypeId });}
</div>

<div class="row">
    <h4>Vehicle Damage History</h4>
    @{Html.RenderAction("ViewRepair", new { id = Model.Id });}
</div>


